    function loadCentreSearchResults() {
        $.ajax({ url: "/esolfacsearch/updatesearchcriteria/",
            cache: false,
            type: "post",
            data: $('.centre-search-criteria').find('input, select').serialize(),
            success: function (response) {
                $('.result-container').html(response);
                showSearchResults();

                $('.contact-details').click(function () {
                    loadCentreDetails(this);
                    return false;
                });

                $('.accordion').accordion({
                    collapsible: true,
                    autoHeight: false,
                    heightStyle: "content",
                    navigation: true,
                    active: false
                });

            },
            error: function (error) {
                $('.result-container').html(error);
            }
        });

        linkBackToResultsButton();
    }

    function loadCentreDetails(control) {
        $.ajax({ url: "/findacentre/viewcentredetails/",
            cache: false,
            type: "post",
            data: "centreId=" + $(control).attr("id"),
            success: function (response) {
                hideSearchResults(response);
            },
            error: function (error) {
                $('.centre-details-data').html(error);
            }
        });
    }

    function linkBackToResultsButton() {
        $('.back-to-results').click(function () {
            showSearchResults();
        });
    }

    function showSearchResults() {
        $('.centre-details-data').html("");
        $('.result-container').show();
        $('.additional-content').show();
        $('.back-to-results').addClass('display-none');
    }

    function hideSearchResults(response) {
        $('.result-container').hide();
        $('.additional-content').hide();
        $('.centre-details-data').html(response);
        $('.back-to-results').removeClass('display-none');
    }

</script>



